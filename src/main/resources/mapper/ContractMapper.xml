<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weng.contractmanagementbackend.mapper.ContractMapper">

    <resultMap id="BaseResultMap" type="com.weng.contractmanagementbackend.model.domain.Contract">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="signatory" column="signatory" jdbcType="VARCHAR"/>
            <result property="signatureTime" column="signature_time" jdbcType="TIMESTAMP"/>
            <result property="status" column="status" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="DATE"/>
            <result property="updateTime" column="update_time" jdbcType="DATE"/>
            <result property="deleted" column="deleted" jdbcType="TINYINT"/>
    </resultMap>

</mapper>
